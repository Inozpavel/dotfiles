{
	"hyprland/workspaces": {
		"on-scroll-up": "hyprctl dispatch workspace r-1",
		"on-scroll-down": "hyprctl dispatch workspace r+1",
		"on-click": "activate",
		"active-only": false,
		"all-outputs": true,
		"show-special": true,
		"format": "{}",
		"format-icons": {
			"urgent": "",
			"active": "",
			"default": ""
		},
		"sort-by-numer": true
	},
	"hyprland/window": {
		"rewrite": {
			"(.*) - Brave": "$1",
			"(.*) - Chromium": "$1",
			"(.*) - Brave Search": "$1",
			"(.*) - Outlook": "$1",
			"(.*) Microsoft Teams": "$1"
		},
		"separate-outputs": true
	},
	"hyprland/language": {
		"format": "ï„œ   {}",
		"format-en": "US",
		"format-ru": "RU"
	}
}
